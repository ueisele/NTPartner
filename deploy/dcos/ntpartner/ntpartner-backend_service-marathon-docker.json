{
  "id": "/ntpartner/backend",
  "instances": 1,
  "cpus": 0.5,
  "mem": 1024,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nexus3.marathon.l4lb.thisdcos.directory:5000/ntpartner/ntpartner-backend",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 0,
          "protocol": "tcp",
          "name": "http-api",
          "labels": {
            "VIP_0": "/ntpartner/backend:80"
          }
        }
      ]
    }
  },
  "env": {
    "MYSQL_HOST": "ntpartnermysql.marathon.l4lb.thisdcos.directory",
    "MYSQL_DATABASE": "ntpartner",
    "MYSQL_USER": "dbuser",
    "MYSQL_PASSWORD": "dbuser",
    "RABBITMQ_HOST": "ntpartnerrabbitmq.marathon.l4lb.thisdcos.directory",
    "RABBITMQ_VHOST": "ntpartner",
    "RABBITMQ_USER": "mquser",
    "RABBITMQ_PASSWORD": "mquser"
  },
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "ntpartner.nt.de",
    "HAPROXY_0_PORT": "8081",
    "de.nt.cloud.group": "ntpartner",
    "de.nt.cloud.name": "ntpartner-backend"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/v1/partner",
      "portName": "http-api",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 30,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "readinessChecks": [
    {
      "protocol": "HTTP",
      "path": "/v1/partner",
      "portName": "http-api",
      "intervalSeconds": 60,
      "timeoutSeconds": 30,
      "httpStatusCodesForReady": [ 200, 302 ],
      "preserveLastResponse": true
    }
  ],
  "uris": [
    "http://boot.dcos/docker/nexus3.marathon.l4lb.thisdcos.directory:5000/docker.tar.gz"
  ]
}